TARGET_NAME=libz
ifeq "$(XDK_TARGET_PRODUCT)" "devtools"
ifeq "$(XDK_TARGET_PLATFORM)" "win32"
TARGET_TYPE=eco
else
TARGET_TYPE=so
endif
else
TARGET_TYPE=eco
endif

ifeq "$(XDK_TARGET_PRODUCT)" "devtools"
  C_DEFINES+=-DNO_VIZ
endif

ifeq "$(XDK_TARGET_PLATFORM)" "win32"
  C_DEFINES+=-UWIN32
  C_DEFINES+=-U_WIN32
endif

EXPORT_HEADERS+=zlib.h
EXPORT_HEADERS+=zconf.h

ifeq "$(XDK_TARGET_PLATFORM)" "win32"
SOURCES += zlib.def
endif
SOURCES += adler32.c
