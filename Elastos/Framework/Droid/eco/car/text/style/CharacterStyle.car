
module
{
    interface ITextPaint;

    interface ICharacterStyle {
        UpdateDrawState(
            [in] ITextPaint* tp);

        /**
         * Returns "this" for most CharacterStyles, but for CharacterStyles
         * that were generated by {@link #wrap}, returns the underlying
         * CharacterStyle.
         */
        GetUnderlying(
            [out] ICharacterStyle** result);
    }
}
