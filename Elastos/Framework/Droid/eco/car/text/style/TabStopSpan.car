
module
{
    interface IParagraphStyle;

    /**
     * Represents a single tab stop on a line.
     */
    interface ITabStopSpan extends IParagraphStyle
    {
        /**
         * Returns the offset of the tab stop from the leading margin of the
         * line.
         * @return the offset
         */
        GetTabStop(
            [out] Int32* offset);
    }
}
