
module
{
    /*
     * Handles the audio error callback.
     */
    interface IAudioSystemErrorCallback {
        /*
         * Callback for audio server errors.
         * param error   error code:
         * - AUDIO_STATUS_OK
         * - AUDIO_STATUS_SERVER_DIED
         * - AUDIO_STATUS_ERROR
         */
        OnError(
            [in] Int32 error);
    }
}
