    interface ISettingsSecure {
        GetString(
            [in] IContentResolver* resolver,
            [in] String name,
            [out] String* outString);

        PutString(
            [in] IContentResolver* resolver,
            [in] String name,
            [in] String value,
            [out] Boolean* outBoolean);


        GetUriFor(
            [in] String name,
            [out] IUri** outUri);

        GetInt(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int32 def,
            [out] Int32* outInt);

        GetIntExtra(
            [in] IContentResolver* cr,
            [in] String name,
            [out] Int32* outInt);

        PutInt(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int32 value,
            [out] Boolean* outBoolean);

        GetLong(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int64 def,
            [out] Int64* outLong);

        GetLongExtra(
            [in] IContentResolver* cr,
            [in] String name,
            [out] Int64* outLong);

        PutLong(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int64 value,
            [out] Boolean* outBoolean);

        GetFloat(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Float def,
            [out] Float* outFloat);

        GetFloatExtra(
            [in] IContentResolver* cr,
            [in] String name,
            [out] Float* outFloat);

        PutFloat(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Float value,
            [out] Boolean* outBoolean);


        GetBluetoothHeadsetPriorityKey(
            [in] String address,
            [out] String* outString);

        GetBluetoothA2dpSinkPriorityKey(
            [in] String address,
            [out] String* outString);

        IsLocationProviderEnabled(
            [in] IContentResolver* cr,
            [in] String provider,
            [out] Boolean *outBoolean);

        SetLocationProviderEnabled(
            [in] IContentResolver* cr,
            [in] String provider,
            [in] Boolean enabled);

    }
