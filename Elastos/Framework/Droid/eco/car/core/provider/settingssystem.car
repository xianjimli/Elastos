    interface ISettingsSystem {

        GetString(
            [in] IContentResolver* resolver,
            [in] String name,
            [out] String* outString);

        PutString(
            [in] IContentResolver* resolver,
            [in] String name,
            [in] String value,
            [out] Boolean* outBoolean);


        GetUriFor(
            [in] String name,
            [out] IUri** outUri);

        GetInt(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int32 def,
            [out] Int32* outInt);

        GetIntExtra(
            [in] IContentResolver* cr,
            [in] String name,
            [out] Int32* outInt);

        PutInt(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int32 value,
            [out] Boolean* outBoolean);

        GetLong(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int64 def,
            [out] Int64* outLong);

        GetLongExtra(
            [in] IContentResolver* cr,
            [in] String name,
            [out] Int64* outLong);

        PutLong(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Int64 value,
            [out] Boolean* outBoolean);

        GetFloat(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Float def,
            [out] Float* outFloat);

        GetFloatExtra(
            [in] IContentResolver* cr,
            [in] String name,
            [out] Float* outFloat);

        PutFloat(
            [in] IContentResolver* cr,
            [in] String name,
            [in] Float value,
            [out] Boolean* outBoolean);


        GetConfiguration(
            [in] IContentResolver* cr,
            [in] IConfiguration* outConfig);

        PutConfiguration(
            [in] IContentResolver* cr,
            [in] IConfiguration* config,
            [out] Boolean* outBoolean);

        HasInterestingConfigurationChanges(
            [in] Int32 changes,
            [out] Boolean* outBoolean);

        GetShowGTalkServiceStatus(
            [in] IContentResolver* cr,
            [out] Boolean* outBoolean);

        SetShowGTalkServiceStatus(
            [in] IContentResolver* cr,
            [in] Boolean flag);

