module
{
    const IBulkCursor_GET_CURSOR_WINDOW_TRANSACTION = 1;
    const IBulkCursor_COUNT_TRANSACTION = 2;
    const IBulkCursor_GET_COLUMN_NAMES_TRANSACTION = 3;
    const IBulkCursor_UPDATE_ROWS_TRANSACTION = 4;
    const IBulkCursor_DELETE_ROW_TRANSACTION = 5;
    const IBulkCursor_DEACTIVATE_TRANSACTION = 6;
    const IBulkCursor_REQUERY_TRANSACTION = 7;
    const IBulkCursor_ON_MOVE_TRANSACTION = 8;
    const IBulkCursor_WANTS_ON_MOVE_TRANSACTION = 9;
    const IBulkCursor_GET_EXTRAS_TRANSACTION = 10;
    const IBulkCursor_RESPOND_TRANSACTION = 11;
    const IBulkCursor_CLOSE_TRANSACTION = 12;

    interface ICursorWindow;
    interface IContentObserver;
    interface IBundle;

    /**
     * This interface provides a low-level way to pass bulk cursor data across
     * both process and language boundries. Application code should use the Cursor
     * interface directly.
     *
     * {@hide}
     */
    interface IBulkCursor
    {
        /**
         * Returns a BulkCursorWindow, which either has a reference to a shared
         * memory segment with the rows, or an array of JSON strings.
         */
        GetWindow(
            [in] Int32 startPos,
            [out] ICursorWindow** cw);

        OnMove(
            [in] Int32 position);

        /**
         * Returns the number of rows in the cursor.
         */
        Count(
            [out] Int32* count);

        /**
         * Returns a string array holding the names of all of the columns in the
         * cursor in the order in which they were listed in the result.
         */
        GetColumnNames(
            [out,callee] ArrayOf<String>* names);

        //public boolean updateRows(Map<? extends Long, ? extends Map<String, Object>> values) throws RemoteException;
        //UpdateRows(
        //    [in] Map<Int64, Map<String, IInterface*> > values,
        //    [out] Boolean* result);

        DeleteRow(
            [in] Int32 position,
            [out] Boolean* succeeded);

        Deactivate();

        Close();

        Requery(
            [in] IContentObserver* observer,
            [in] ICursorWindow* window,
            [out] Int32* value);

        GetWantsAllOnMoveCalls(
            [out] Boolean* result);

        GetExtras(
            [out] IBundle** extras);

        Respond(
            [in] IBundle* extras,
            [out] IBundle** result);
    }
}