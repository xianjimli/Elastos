
module
{
    interface ICanvas;
    interface IPaint;

    interface IMovie {
        Width(
            [out] Int32* width);

        Height(
            [out] Int32* height);

        IsOpaque(
            [out] Boolean* isOpaque);

        Duration(
            [out] Int32* height);

        SetTime(
            [in] Int32 relativeMilliseconds,
            [out] Boolean* isSet);

        Draw(
            [in] ICanvas* canvas,
            [in] Float x,
            [in] Float y,
            [in] IPaint* paint);

        DrawEx(
            [in] ICanvas* canvas,
            [in] Float x,
            [in] Float y);
    }

    interface IMovieHelper {
        DecodeFile(
            [in] String pathName,
            [out] IMovie ** Movie);

        DecodeStream(
            [in] IInputStream * pIs,
            [out] IMovie ** Movie);

        DecodeByteArray(
            [in] ArrayOf<Byte> data,
            [in] Int32 offset,
            [in] Int32 length,
            [out] IMovie ** Movie);
    }
}
