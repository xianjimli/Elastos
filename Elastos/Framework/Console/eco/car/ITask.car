
module
{
    enum TaskFinish {
        TaskFinish_Nice,
        TaskFinish_ASAP,
    }

    interface ITask {
        Start(
            [in] BufferOf<CString> args);

        Finish(
            [in] TaskFinish flag);
    }

    interface ITaskEntry {
        Main(
            [in] BufferOf<CString> args);
    }

    callbacks JTaskEvents {
        Finished();
    }
}