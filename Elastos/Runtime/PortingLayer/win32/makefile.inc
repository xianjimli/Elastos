$(SPECIAL_TARGET):
##########################################################
# copy export head files generated by *.car to system\inc
##########################################################
#created by .car files
	@echo Copying dbus-1.lib to XDK_USER_LIB...
	$(V) $(CP) $(MAKEDIR)\dbus\dbus-1.lib $(XDK_USER_LIB) $(BLACKHOLE)
	@echo Copying dir dbus.lib to XDK_USER_INC...
	if not exist $(XDK_USER_INC)\dbus mkdir $(XDK_USER_INC)\dbus
	$(V) $(CP) $(MAKEDIR)\dbus\dbus*.h $(XDK_USER_INC)\dbus $(BLACKHOLE)

	@touch $@

